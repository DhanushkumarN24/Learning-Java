// User function Template for Java

class Solution {
    public List<Integer> valueEqualToIndex(List<Integer> nums) {
        ArrayList<Integer> list = new ArrayList<>();
        for(int i=0;i<nums.size();i++){
            if(i+1==nums.get(i)){
                list.add(i+1);
            }
        }
        return list;
    }
}
