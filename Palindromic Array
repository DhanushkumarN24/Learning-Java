class Solution {
    public static boolean isPalinArray(int[] arr) {
        for(int i=0;i<arr.length;i++){
            StringBuilder sb = new StringBuilder(Integer.toString(arr[i]));
            sb.reverse();
            int rev = Integer.parseInt(sb.toString());
            if(arr[i] != rev){
                return false;
            }
        }
        return true;
    }
}
